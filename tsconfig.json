{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"noImplicitAny": false,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"types": [
			"node",
			"webpack-env" // here
		]
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.mdx"],
	"exclude": ["node_modules"]
}
